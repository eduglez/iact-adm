    <navigation-rule>
        <from-view-id>/DatosAuxiliares/Departamentos/*</from-view-id>
        <navigation-case>
            <from-outcome>departamento-insertar</from-outcome>
            <to-view-id>/DatosAuxiliares/Departamentos/insertar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>departamento-editar</from-outcome>
            <to-view-id>/DatosAuxiliares/Departamentos/editar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

