    </navigation-rule>

   

    <navigation-rule>
        <from-view-id>/personal/*</from-view-id>

        <navigation-case>
            <from-outcome>empleado-editar</from-outcome>
            <to-view-id>/personal/editar/</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>




