<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">
  <context-root>/iact-adm</context-root>
  <security-role-mapping>
    <role-name>gerencia</role-name>
    <group-name>gerencia</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>adm</role-name>
    <group-name>adm</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>secretaria</role-name>
    <group-name>secretaria</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>servicios</role-name>
    <group-name>servicios</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>compras</role-name>
    <group-name>compras</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>investigador</role-name>
    <group-name>investigador</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>personal</role-name>
    <group-name>personal</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>proyectos</role-name>
    <group-name>proyectos</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>culturacientifica</role-name>
    <group-name>culturacientifica</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</sun-web-app>
